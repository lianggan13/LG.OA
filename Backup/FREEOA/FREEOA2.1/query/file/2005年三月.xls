<title>系统信息</title>
</head>
<script language="javascript" type="text/javascript" src="http://mimg.163.com/netfolder/js/netease.js"></script>
<script language="javascript" type="text/javascript" src="http://mimg.163.com/netfolder/js/netfolder.js"></script>
<script language="javascript" type="text/javascript" src="http://mimg.163.com/netfolder/js/netfolder_res_zh.js"></script>
<script language="javascript" type="text/javascript"><!--
function ResetDomain()
{	var ss=document.domain;						//ss == "www.126.com";
	var ii=ss.lastIndexOf('.');
	if(ii>0)
	{	if(!isNaN(ss.substr(ii+1)*1))
			return;
		ii=ss.lastIndexOf('.',ii-1);
		if(ii>0)
			document.domain	=ss.substr(ii+1);
	}											//document.domain == "126.com";
}
if(location.href.indexOf("wp.mail")>-1){
	ResetDomain();
}
function submitBack() {
	var oBackurl = document.getElementById("backurl");
    if (oBackurl.value && oBackurl.value !="") {
		var win = window.top.frames["jsFrame"];
		if(!!win && oBackurl.value.indexOf("http://wp") == -1){
			if(document.getElementById("message").innerHTML == "文件保存成功！"){
				if(win.MM["read"].backFromNetFolder){
					win.MM["read"].backFromNetFolder(window);
				}else{
					win.MM["read"].cmURL = oBackurl.value;
					win.MM.getModule("read");
				}
			}
		}else{
			if(!document.all&&location.href.toString().indexOf("PostMailPart")>-1&&document.getElementById("message").innerHTML == "文件保存成功！"){
				if(window.top.frames["justfresh"]){ // 简约3 
					if(backurl.value.indexOf("history_go") == -1){
						location.href = oBackurl.value;
					}else{
						if(oBackurl.value.indexOf("history_go") > -1){
							history.go(oBackurl.value.replace(/.+history_go=/,""));
						}else{
							history.back();
						}
					}
				}else{
					var win = window.top.frames["index"];
					win.MM.getModule("read");
					
				}
				return;
			}
			if(backurl.value.indexOf("history_go") == -1){
	        location.href = oBackurl.value;
			}else{
				if(oBackurl.value.indexOf("history_go") > -1){
					history.go(oBackurl.value.replace(/.+history_go=/,""));
				}else{
					history.back();
				}
			}
		}
    } else {
        history.back();
    }
}

function init() {
    if (document.body.clientHeight < 10 || document.body.clientHeight < 10) {
        // check if in a hidden frame
        alert(head.innerHTML + "\n\n" + message.innerHTML + "\n\n" + detail.innerHTML);
        if (backurl.value) {
            parent.main.location.href = backurl.value;
            // parent.main.location.reload() ?? 
        }
    }
}
//--></script>
<body onLoad="init()">
<input id="backurl" type="hidden" value="">
<table width="100%" height="100%" border="0" cellspacing="0" cellpadding="0" class="twp_all">
  <tr>
    <td class="twp_head_simp"><div class="wp_toolbar">
        <div class="wp_toolbar_inner">
          <!--button begin-->
          <div class="wp_btn wp_btn1"> <a href="javascript:submitBack();"> <span class="wp_btn_inner"> <b class="btn_allimg btn_ico_13"></b><span class="btn_text">返回</span> </span> </a> </div>
          <!--button end-->
          <!--button begin-->
          <div class="wp_btn wp_btn3"> <a href="main_list.html?sid="> <span class="wp_btn_inner"> <b class="btn_allimg btn_ico_8"></b><span class="btn_text">网盘首页</span> </span> </a> </div>
          <!--button end-->
          <!--button begin-->
          <div class="wp_btn wp_btn3"> <a href="main_search.html?sid="> <span class="wp_btn_inner"> <b class="btn_allimg btn_ico_9"></b> <span class="btn_text">搜索文件</span> </span> </a> </div>
          <!--button end-->
          <!--button begin-->
          <div class="wp_btn wp_btn3"> <a href="main_quota.html?sid="> <span class="wp_btn_inner"> <b class="btn_allimg btn_ico_6"></b> <span class="btn_text">查看容量</span> </span> </a> </div>
          <!--button end-->
        </div>
      </div></td>
  </tr>
  <tr>
    <td class="twp_con twp_con_sysinfo"><!--列表 开始-->
      <table border="0" cellspacing="0" cellpadding="0" class="tbl_oplist_wp wp_sys_info">
        <tr class="tbl_oplist_inner_title">
          <td class="oplist_inner_title_l" id="head">系统信息</td>
        </tr>
        <tr>
          <td><div class="font_notice sys_info_con">
              <p><img src="http://mimg.163.com/netfolder/icons/notice.gif" width="16" height="16">&#160;<span id="message">用户非法登陆！</span></p>
              <p id="detail"></p>
            </div>
          <div class="sys_info_buttonbar"><input type="Button" value="确定" onclick="submitBack()" name="button" class="input_button" /></div>
          </td>
        </tr>
      </table></td>
  </tr>
  <tr>
    <td class="wp_bottom wp_bottom_info"><iframe width="100%" height="18" scrolling="no" frameborder="0" src="main_info.html?sid="></iframe></td>
  </tr>
</table>
</body>
</html>
